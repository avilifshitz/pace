<h1>ALL USER</h1>
<table>
	<thead>
		<tr>
			<th scope="col">&nbsp;</th>
			<th scope="col">ID</th>
			<th scope="col">EMAIL</th>
			<th scope="col">Created at</th>
			
			<th scope="col" >Modify</th>
		</tr>
	</thead>

		<% @users.each do |user| %>
		<%#= debug user %>
	    <tr>
			<td class="align-center">&nbsp;</td>
			<td><%= user.id %></td>
			<td><%= user.email %></td>
			<td><%= user.created_at%></td>
			<td>
				
				<%= link_to "Edit" , edit_admin_dashboard_path(user.id) %> |
				<%= link_to "Show",admin_dashboard_path(user) %> |
				<%= link_to "Delete", admin_destroy_path(user), data: { confirm: 'Are you sure ?:' }%>
			</td>
		</tr>
			
			
	<% end %>